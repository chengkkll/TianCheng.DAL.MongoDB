{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/common-{Date}.txt",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://samples:samplesPwd@localhost:27017/samples",
          "restrictedToMinimumLevel": "Warning",
          "collectionName": "system_log"
        }
      }
    ]
  },
  "DBConnection": [
    {
      "Name": "default",
      "ServerAddress": "mongodb://localhost:27017",
      "Database": "samples",
      "Type": "MongoDB"
    },
    {
      "Name": "debug",
      "ServerAddress": "mongodb://localhost:27017",
      "Database": "rt_test",
      "Type": "MongoDB"
    }
  ]
}
